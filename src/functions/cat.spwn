extract $

cat = (context){
	extract context

    let curRFile = 0
	if args.length >= 1 {
	    while curRFile < args.length {
			print(readfile(args[curRFile]))
			curRFile++
		}
		curRFile = 0
    }
}

return @command::new(
	name = 'cat',
	description = 'Displays contents of a file.',
	call = cat,
)
